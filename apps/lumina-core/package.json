{
  "name": "@lumina/core",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "20.x",
    "npm": "^10.x"
  },
  "scripts": {
    "dev": "next dev -p 3002",
    "build": "NODE_ENV=production npm run build:tokens && next build",
    "build:tokens": "node ./src/theme/build.js",
    "start": "NODE_ENV=production next start",
    "check-types": "tsc --noEmit",
    "lint": "npm run lint:sass && npm run lint:css && npm run lint:tokens && next lint",
    "lint:sass": "stylelint --fix --allow-empty-input \"**/*.scss\"",
    "lint:css": "stylelint --fix --allow-empty-input \"./dist/index.css\" \"./dist/reset.css\"",
    "lint:tokens": "stylelint --fix --allow-empty-input \"./dist/tokens.css\"",
    "docs": "docsify serve ./docs"
  },
  "dependencies": {
    "next": "14.2.4",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-modal": "^3.16.1",
    "react-select": "^5.8.0",
    "url-loader": "^4.1.1"
  },
  "devDependencies": {
    "@types/node": "^20.14.2",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@types/react-modal": "^3.16.3",
    "@types/react-select": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "classnames": "^2.5.1",
    "docsify-cli": "^4.4.4",
    "dotenv": "^16.4.5",
    "eslint": "8.46.0",
    "eslint-config-next": "^12.3.1",
    "eslint-config-prettier": "^8.10.0",
    "eslint-config-turbo": "^2.0.4",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.3.2",
    "stylelint": "^16.6.1",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-recess-order": "^5.0.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "style-dictionary": "^3.9.2",
    "sass": "^1.77.5",
    "typescript": "5.4.5"
  }
}
