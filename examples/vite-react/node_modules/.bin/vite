#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/bmirra01/Repo/lumina-next/node_modules/.pnpm/vite@5.3.5_@types+node@20.11.24_sass@1.77.8_terser@5.31.3/node_modules/vite/bin/node_modules:/Users/bmirra01/Repo/lumina-next/node_modules/.pnpm/vite@5.3.5_@types+node@20.11.24_sass@1.77.8_terser@5.31.3/node_modules/vite/node_modules:/Users/bmirra01/Repo/lumina-next/node_modules/.pnpm/vite@5.3.5_@types+node@20.11.24_sass@1.77.8_terser@5.31.3/node_modules:/Users/bmirra01/Repo/lumina-next/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/bmirra01/Repo/lumina-next/node_modules/.pnpm/vite@5.3.5_@types+node@20.11.24_sass@1.77.8_terser@5.31.3/node_modules/vite/bin/node_modules:/Users/bmirra01/Repo/lumina-next/node_modules/.pnpm/vite@5.3.5_@types+node@20.11.24_sass@1.77.8_terser@5.31.3/node_modules/vite/node_modules:/Users/bmirra01/Repo/lumina-next/node_modules/.pnpm/vite@5.3.5_@types+node@20.11.24_sass@1.77.8_terser@5.31.3/node_modules:/Users/bmirra01/Repo/lumina-next/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
