{
  "name": "@lumina/client-demo",
  "private": true,
  "version": "0.0.1",
  "description": "Lumina client demo webapp",
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "npm run lint:js && npm run lint:css && npm run prettier",
    "lint:js": "eslint ./src --fix --report-unused-disable-directives --max-warnings 10",
    "lint:css": "stylelint --fix \"**/*.{css,scss}\" --config ../../.stylelintrc.json",
    "prettier": "prettier --write ./src",
    "preview": "vite preview"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@fontsource/roboto": "^5.0.14",
    "@mui/icons-material": "^6.0.2",
    "@mui/material": "^6.0.2",
    "classnames": "^2.5.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-responsive-carousel": "^3.2.23",
    "react-router-dom": "^6.26.1"
  },
  "devDependencies": {
    "@lumina/core": "workspace:*",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "globals": "^15.8.0",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.3",
    "stylelint": "^16.8.1",
    "typescript": "^5.2.2",
    "vite": "^5.3.4"
  },
  "lint-staged": {
    "*.{scss,css}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --quiet --fix",
      "prettier --write"
    ],
    "!(*.scss|*.css|*.js|*.js|*.js|*.js|*.png|*.svg|*.jpg|*.eot|*.svg|*.ttf|*.woff)": [
      "prettier --write"
    ]
  }
}
