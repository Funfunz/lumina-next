#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/bmirra01/Repo/lumina-next/packages/lumina-core/node_modules/strip-ansi/node_modules:/Users/bmirra01/Repo/lumina-next/packages/lumina-core/node_modules:/Users/bmirra01/Repo/lumina-next/packages/node_modules:/Users/bmirra01/Repo/lumina-next/node_modules:/Users/bmirra01/Repo/node_modules:/Users/bmirra01/node_modules:/Users/node_modules:/node_modules:/Users/bmirra01/Repo/lumina-next/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/bmirra01/Repo/lumina-next/packages/lumina-core/node_modules/strip-ansi/node_modules:/Users/bmirra01/Repo/lumina-next/packages/lumina-core/node_modules:/Users/bmirra01/Repo/lumina-next/packages/node_modules:/Users/bmirra01/Repo/lumina-next/node_modules:/Users/bmirra01/Repo/node_modules:/Users/bmirra01/node_modules:/Users/node_modules:/node_modules:/Users/bmirra01/Repo/lumina-next/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../strip-ansi/cli.js" "$@"
else
  exec node  "$basedir/../strip-ansi/cli.js" "$@"
fi
