#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/bmirra01/Repo/lumina-next/node_modules/.pnpm/watch-cli@0.2.3/node_modules/watch-cli/bin/node_modules:/Users/bmirra01/Repo/lumina-next/node_modules/.pnpm/watch-cli@0.2.3/node_modules/watch-cli/node_modules:/Users/bmirra01/Repo/lumina-next/node_modules/.pnpm/watch-cli@0.2.3/node_modules:/Users/bmirra01/Repo/lumina-next/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/bmirra01/Repo/lumina-next/node_modules/.pnpm/watch-cli@0.2.3/node_modules/watch-cli/bin/node_modules:/Users/bmirra01/Repo/lumina-next/node_modules/.pnpm/watch-cli@0.2.3/node_modules/watch-cli/node_modules:/Users/bmirra01/Repo/lumina-next/node_modules/.pnpm/watch-cli@0.2.3/node_modules:/Users/bmirra01/Repo/lumina-next/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../watch-cli/bin/watch" "$@"
else
  exec node  "$basedir/../watch-cli/bin/watch" "$@"
fi
